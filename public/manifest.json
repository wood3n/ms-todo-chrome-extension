{
  "manifest_version": 3,
  "name": "MS Todo",
  "description": "Manage your Microsoft To Do data",
  "version": "1.0",
  "action": {
    "default_icon": {
      "16": "extension_icon@16px.png",
      "32": "extension_icon@32px.png",
      "48": "extension_icon@48px.png",
      "128": "extension_icon@128px.png"
    }
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": ["identity", "storage", "notifications", "sidePanel", "alarms"],
  "icons": {
    "16": "extension_icon@16px.png",
    "32": "extension_icon@32px.png",
    "48": "extension_icon@48px.png",
    "128": "extension_icon@128px.png"
  },
  "host_permissions": ["https://*/*", "http://*/*"],
  "minimum_chrome_version": "120"
}
